# Public key for Snipcart (obtain from Snipcart dashboard -> Account -> API keys)
SNIPCART_PUBLIC_KEY=your-snipcart-public-key
# Optional: override default currency (defaults to USD)
# SNIPCART_CURRENCY=USD

# Form endpoints
# FORMSPREE_ENDPOINT=https://formspree.io/f/your-id
# NEWSLETTER_ACTION=https://your-email-provider.com/subscribe

# Analytics
# PLAUSIBLE_DOMAIN=yourdomain.com

# Localization
# SITE_LOCALE=en
# SITE_ALT_LOCALES=es

# Machine translation (optional)
# TRANSLATION_PROVIDER=deepl
# DEEPL_API_KEY=your-deepl-key
# DEEPL_SOURCE_LANG=EN
